{
  "ali-dev": {
    "nav": {
      "padding-block-start": {
        "$type": "dimension",
        "$value": "{ali-dev.spacing.sm}"
      },
      "padding-block-end": {
        "$type": "dimension",
        "$value": "{ali-dev.spacing.sm}"
      },
      "link": {
        "padding-block-end": {
          "$type": "dimension",
          "$value": "{ali-dev.spacing.sm}"
        },
        "color": {
          "$type": "color",
          "$value": "{ali-dev.colors.grey.0}"
        },
        "active": {
          "color": {
            "$type": "color",
            "$value": "{ali-dev.colors.primary}"
          },
          "border-block-end-color": {
            "$type": "color",
            "$value": "{ali-dev.colors.primary}"
          },
          "border-block-end-width": {
            "$type": "dimension",
            "$value": "2px"
          }
        },
        "hover": {
          "color": {
            "$type": "color",
            "$value": "{ali-dev.colors.primary-dark}"
          },
          "border-block-end-width": {
            "$type": "dimension",
            "$value": "2px"
          },
          "border-block-end-color": {
            "$type": "color",
            "$value": "{ali-dev.colors.primary-dark}"
          }
        },
        "font-family": {
          "$type": "font",
          "$value": "{ali-dev.typography.font-family.sans}"
        }
      },
      "list": {
        "mobile": {
          "padding-block-start": {
            "$type": "dimension",
            "$value": "{ali-dev.spacing.lg}"
          },
          "background-color": {
            "$type": "color",
            "$value": "{ali-dev.colors.secondary-bg}"
          },
          "row-gap": {
            "$type": "dimension",
            "$value": "{ali-dev.spacing.xxl}"
          },
          "inline-size": {
            "$type": "dimension",
            "$value": "50%"
          }
        },
        "padding-block-start": {
          "$type": "dimension",
          "$value": "{ali-dev.spacing.md}"
        },
        "padding-block-end": {
          "$type": "dimension",
          "$value": "{ali-dev.spacing.md}"
        },
        "column-gap": {
          "$type": "dimension",
          "$value": "{ali-dev.spacing.lg}"
        }
      },
      "button": {
        "font-size": {
          "$type": "dimension",
          "$value": "2rem"
        },
        "color": {
          "$type": "color",
          "$value": "{ali-dev.colors.grey.0}"
        },
        "hover": {
          "color": {
            "$type": "color",
            "$value": "{ali-dev.colors.primary-dark}"
          }
        }
      }
    }
  }
}
