{
  "ali-dev": {
    "toc": {
      "background-color": {
        "$type": "color",
        "$value": "{ali-dev.colors.primary-bg}"
      },
      "border-radius": {
        "$type": "dimension",
        "$value": "5px"
      },
      "body": {
        "padding-inline-start": {
          "$type": "dimension",
          "$value": "{ali-dev.spacing.3xl}"
        },
        "padding-inline-end": {
          "$type": "dimension",
          "$value": "{ali-dev.spacing.3xl}"
        },
        "padding-block-start": {
          "$type": "dimension",
          "$value": "{ali-dev.spacing.xs}"
        },
        "padding-block-end": {
          "$type": "dimension",
          "$value": "{ali-dev.spacing.xl}"
        },
        "max-block-size": {
          "$type": "dimension",
          "$value": "400px"
        }
      },
      "header": {
        "padding-block-start": {
          "$type": "dimension",
          "$value": "{ali-dev.spacing.md}"
        },
        "padding-block-end": {
          "$type": "dimension",
          "$value": "{ali-dev.spacing.md}"
        },
        "padding-inline-start": {
          "$type": "dimension",
          "$value": "{ali-dev.spacing.xl}"
        },
        "padding-inline-end": {
          "$type": "dimension",
          "$value": "{ali-dev.spacing.xl}"
        }
      },
      "list": {
        "margin-block-start": {
          "$type": "dimension",
          "$value": "{ali-dev.spacing.md}"
        },
        "margin-block-end": {
          "$type": "dimension",
          "$value": "{ali-dev.spacing.md}"
        }
      },
      "item": {
        "margin-block-start": {
          "$type": "dimension",
          "$value": "{ali-dev.spacing.md}"
        }
      },
      "link": {
        "underline-offset": {
          "$type": "dimension",
          "$value": "5px"
        },
        "font-size": {
          "$type": "dimension",
          "$value": "{ali-dev.typography.font-size.lg}"
        }
      }
    }
  }
}
