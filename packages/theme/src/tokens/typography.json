{
  "ali-dev": {
    "typography": {
      "font-family": {
        "sans": {
          "$type": "font",
          "$value": "\"Roboto\", sans-serif"
        },
        "serif": {
          "$type": "font",
          "$value": "\"Times New Roman\", serif"
        }
      },
      "font-weight": {
        "regular": {
          "$type": "fontWeight",
          "$value": 400
        },
        "medium": {
          "$type": "fontWeight",
          "$value": 500
        },
        "bold": {
          "$type": "fontWeight",
          "$value": 700
        }
      },
      "font-size": {
        "sm": {
          "$type": "dimension",
          "$value": "14px"
        },
        "md": {
          "$type": "dimension",
          "$value": "16px"
        },
        "lg": {
          "$type": "dimension",
          "$value": "20px"
        },
        "xl": {
          "$type": "dimension",
          "$value": "24px"
        },
        "h1": {
          "$type": "dimension",
          "$value": "40px"
        },
        "h2": {
          "$type": "dimension",
          "$value": "32px"
        },
        "h3": {
          "$type": "dimension",
          "$value": "26px"
        },
        "h4": {
          "$type": "dimension",
          "$value": "20px"
        },
        "h5": {
          "$type": "dimension",
          "$value": "16px"
        },
        "h6": {
          "$type": "dimension",
          "$value": "14px"
        },
        "2xl": {
          "$type": "dimension",
          "$value": "32px"
        },
        "3xl": {
          "$type": "dimension",
          "$value": "48px"
        }
      },
      "line-height": {
        "tight": {
          "$type": "dimension",
          "$value": "1.2"
        },
        "normal": {
          "$type": "dimension",
          "$value": "1.5"
        },
        "relaxed": {
          "$type": "dimension",
          "$value": "1.75"
        }
      }
    }
  }
}
